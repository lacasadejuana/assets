{
  "name": "lacasadejuana/negocios-panel",
  "description": "The Laravel Framework.",
  "license": "MIT",
  "type": "project",
  "keywords": [
    "framework",
    "laravel"
  ],
  "require": {
    "php": "~8.0",
    "ajcastro/eager-load-pivot-relations": "^0.2.2",
    "barryvdh/laravel-dompdf": "^1.0",
    "barryvdh/laravel-snappy": "^1.0",
    "blade-ui-kit/blade-ui-kit": "^0.3.4",
    "cmixin/business-day": "^1.15",
    "doctrine/dbal": "^3.2",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "illuminate/redis": "^8.65",
    "imliam/laravel-blade-helper": "^1.4",
    "kint-php/kint": "^4.1",
    "lab404/laravel-impersonate": "^1.7",
    "laravel/framework": "^8.65",
    "laravel/horizon": "^5.9",
    "laravel/sanctum": "^2.11",
    "laravel/slack-notification-channel": "^2.4",
    "laravel/telescope": "^4.9",
    "laravel/tinker": "^2.5",
    "laravel/ui": "^3.3",
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/flysystem-cached-adapter": "^1.1",
    "owen-it/laravel-auditing": "^13.0",
    "predis/predis": "^1.1",
    "protoqol/prequel": "^1.23",
    "psr/cache": "^1.0",
    "psr/log": "~1.1.4",
    "qoraiche/laravel-mail-editor": "^3",
    "ramsey/uuid": "~4.2.2",
    "rap2hpoutre/laravel-log-viewer": "^2.2",
    "sentry/sentry-laravel": "^2.13",
    "spatie/laravel-collection-macros": "^7.12",
    "spatie/laravel-flash": "^1.8",
    "spatie/laravel-google-calendar": "^3.5",
    "spatie/laravel-permission": "^5.3",
    "spatie/laravel-schemaless-attributes": "~1.8.3",
    "spatie/macroable": "~1.0.1",
    "staudenmeir/belongs-to-through": "^2.5",
    "staudenmeir/eloquent-eager-limit": "^1.6.0",
    "staudenmeir/eloquent-has-many-deep": "^1.13",
    "symfony/css-selector": "^5",
    "symfony/deprecation-contracts": "^2",
    "symfony/event-dispatcher": "^5",
    "symfony/event-dispatcher-contracts": "^2",
    "symfony/filesystem": "^5",
    "symfony/http-client-contracts": "^2",
    "symfony/options-resolver": "^5",
    "symfony/stopwatch": "^5",
    "symfony/string": "^5",
    "symfony/translation": "^5",
    "symfony/translation-contracts": "^2",
    "unisharp/laravel-filemanager": "dev-patch-1",
    "vinkla/hashids": "*"
  },
  "require-dev": {
    "awssat/tailwindo": "^3.0",
    "barryvdh/laravel-debugbar": "^3.6",
    "barryvdh/laravel-ide-helper": "~2.9",
    "beyondcode/laravel-dump-server": "^1.8",
    "brainmaestro/composer-git-hooks": "^2.8",
    "canvural/phpstan-blade-rule": "^0.4.0",
    "ergebnis/composer-normalize": "^2.23",
    "ergebnis/php-cs-fixer-config": "^3.4",
    "facade/ignition": "^2.17",
    "fakerphp/faker": "^1.9.1",
    "kitloong/laravel-migrations-generator": "^5.2",
    "krlove/eloquent-model-generator": "^1.3",
    "laravel/pint": "^1.1",
    "laraveldaily/larastarters": "^0.2.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^5.10",
    "nunomaduro/larastan": "~1",
    "orangehill/iseed": "^3.0",
    "overtrue/phplint": "^3.0",
    "pestphp/pest-plugin-laravel": "^1.3",
    "php-parallel-lint/php-parallel-lint": "^1.1",
    "phpstan/phpstan": "~1.7.15",
    "phpunit/phpunit": "^9.5.10",
    "phpunitgen/console": "~2",
    "psalm/plugin-laravel": "^1.5",
    "vimeo/psalm": "^4.20"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/rodrigocardenas/laravel-filemanager"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "pestphp/pest-plugin": true
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    },
    "google/apiclient-services": [
      "Drive",
      "Gmail",
      "Calendar",
      "Oauth2"
    ],
    "laravel": {
      "dont-discover": [
        "barryvdh/laravel-debugbar"
      ]
    },
    "unused_hooks": {
      "config": {
        "stop-on-failure": []
      },
      "pre-commit": [
        "composer lint",
        "vendor/bin/pint --test",
        "composer normalize --dry-run"
      ]
    },
    "version": "2.5.0"
  },
  "scripts": {
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "config:cache": "rm bootstrap/cache/*.php && php artisan config:cache",
    "csfixer": "vendor/bin/pint",
    "lint": [
      "phplint -w",
      "@parallel_lint"
    ],
    "local": "APP_ENV=local php artisan config:cache --env=local",
    "parallel_lint": "parallel-lint --exclude vendor --exclude psalm.stubs.php --exclude .build --exclude resources . --exclude .vscode --exclude intelephense-alias.php --exclude _ide_helper.php",
    "phpstan": "phpstan analyse --configuration=phpstan.neon --memory-limit=2G",
    "pretty": "vendor/bin/pint --config pint.json",
    "psalm": "psalm --show-info=false --long-progress --threads=4 --config=psalm.xml ",
    "test": "APP_ENV=testing vendor/bin/phpunit --configuration phpunit.xml --testsuite Unit",
    "tests": [
      "touch .env",
      "APP_ENV=testing php artisan config:cache --env=testing",
      "./vendor/bin/pest  --configuration phpunit.xml --testsuite Unit"
    ]
  }
}
