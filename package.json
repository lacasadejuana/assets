{
	"private": true,
	"name": "assets.juana.house",
	"version": "0.0.1",
	"scripts": {
		"dev": "wrangler pages dev public",
		"build:tailwind": " tailwind  -i src/css/_app.css -o public/css/app.css",
		"build:public": "esbuild src/js/public_map/index.ts --bundle --format=esm --outfile=public/js/public_map.js --tsconfig=tsconfig.json",
		"build:property": "esbuild src/js/property_map/index.ts --bundle --format=esm --outfile=public/js/property_map.js --tsconfig=tsconfig.json",
		"build": "yarn build:public && yarn build:property && yarn build:tailwind",
		"watch:esbuild": "node builders/esbuild.cjs --watch ",
		"watch:tailwind": "tailwind -i src/css/_app.css -o public/property_map/app.css --watch",
		"watch": "concurrently \"npm run watch:esbuild\" \"npm run watch:tailwind\""
	},
